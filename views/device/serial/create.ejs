<div class="modal fade" id="dCreateModal" tabindex="-1">
    <div class="modal-dialog modal-lg">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title">Add Device Information</h5>
                <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
            </div>
            <div class="modal-body">
                <form class="needs-validation" id="dCreate" novalidate enctype="multipart/form-data">
                    <div class="row g-3 mb-1 align-items-center">
                        <!-- Suppliers Price -->
                        <div class="col-auto">
                            <label for="dcSuppliersPrice" class="col-form-label">Suppliers Price</label>
                          </div>
                          <div class="col-4">
                            <input type="text" id="dcSuppliersPrice" class="form-control" required>
                          </div>
                          <!-- Retail Price -->
                          <div class="col-auto">
                            <label for="dcRetailPrice" class="col-form-label">Retail Price</label>
                          </div>
                          <div class="col-4">
                            <input type="text" id="dcRetailPrice" class="form-control" required>
                        </div>
                        <!-- serial -->
                        <div class="col-auto">
                          <label for="dcSerial" class="col-form-label">Serial</label>
                        </div>
                        <div class="col-9">
                          <input type="text" id="dcSerial" class="form-control">
                        </div>
                        <!-- Add Button -->
                        <div class="col-auto">
                            <a id="dcAdd" class="btn btn-success"><i class="bi bi-plus"></i>Add</a>
                        </div>
                    </div>
                    <div class="mb-1">
                        <table id="dcSerialTable">
                            <thead>
                                <tr>
                                    <th data-field="partinformation" data-visible="false">Id</th>
                                    <th data-field="Brand" data-visible="false">Brand</th>
                                    <th data-field="Model" data-visible="false">Model</th>
                                    <th data-field="Description" data-visible="false">Description</th>
                                    
                                </tr>
                            </thead>
                        </table>
                    </div>
                    <div class="modal-footer">
                        <button type="reset" class="btn btn-secondary" data-bs-dismiss="modal">Close</button>
                        <button type="submit" class="btn btn-primary">Save changes</button>
                    </div>
                </form>
            </div>
        </div>
    </div>
</div>


