<!-- tt = Transaction Transportation -->
<div class="modal fade" id="ttAddModal" tabindex="-1">
    <div class="modal-dialog modal-md">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title">Add Transportation</h5>
                <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
            </div>
            <div class="modal-body">
                <form class="needs-validation" id="ttAdd" novalidate enctype="multipart/form-data">
                    <!-- tta = Transaction transportation Add  -->
                    <div class="mb-3">
                        <label for="ttaDescription" class="form-label">Description</label>
                        <input type="text" class="form-control" id="ttaDescription" required>
                    </div>
                    <div class="row">
                        <div class="col-6">
                            <div class="mb-3">
                                <label for="ttaQuantity" class="form-label">Quantity</label>
                                <input type="text" class="form-control" id="ttaQuantity" required>
                            </div>
                        </div>
                        <div class="col-6">
                            <div class="mb-3">
                                <label for="ttaPrice" class="form-label">Price</label>
                                <input type="text" class="form-control" id="ttaPrice" required>
                            </div>
                        </div>
                    </div>

                    <div class="modal-footer">
                        <button type="reset" class="btn btn-secondary" data-bs-dismiss="modal">Close</button>
                        <button type="submit" class="btn btn-primary">Save changes</button>
                    </div>
                </form>
            </div>
        </div>
    </div>
</div>
