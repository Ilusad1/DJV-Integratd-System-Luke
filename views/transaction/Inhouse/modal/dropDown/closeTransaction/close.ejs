<!-- tihvct = Transaction In House View Close Transaction -->
<div class="modal fade" id="tihvctModal" tabindex="-1">
    <div class="modal-dialog modal-md">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title">Close transation</h5>
                <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
            </div>
            <div class="modal-body">
                <form class="needs-validation" id="tihvctUpdate" novalidate enctype="multipart/form-data">
                    <div class="mb-3">
                        <label for="tihvctDate" class="form-label">Date</label>
                        <input type="date" class="form-control" id="tihvctDate" required>
                    </div>
                    <div class="mb-3">
                        <label for="tihvctTechnician" class="form-label">Technician</label>
                        <select class="form-select" id="tihvctTechnician" aria-label="Floating label select example">
                            <option selected>Technician</option>
                            <option value="1">Deancent</option>
                            <option value="2">Vince</option>
                            <option value="3">David</option>
                        </select>
                    </div>
                    
                    <div class="modal-footer">
                        <button type="reset" class="btn btn-secondary" data-bs-dismiss="modal">Close</button>
                        <button type="submit" class="btn btn-primary">Update</button>
                    </div>
                </form>
            </div>
        </div>
    </div>
</div>
        